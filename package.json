{
  "name": "auth0-extension-tools",
  "version": "0.9.0",
  "description": "A set of tools and utilities to simplify the development of Auth0 Extensions.",
  "main": "src/index.js",
  "dependencies": {
    "auth0": "^2.3.1",
    "bluebird": "^3.4.1",
    "jsonwebtoken": "^7.1.7",
    "lodash": "^4.14.1",
    "lru-memoizer": "^1.6.0",
    "ms": "^0.7.1",
    "node-uuid": "^1.4.7",
    "superagent": "^2.2.0",
    "webtask-tools": "^2.2.0"
  },
  "devDependencies": {
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-plugin-import": "^1.12.0",
    "faucet": "0.0.1",
    "mock-fs": "^3.11.0",
    "nock": "^8.0.0",
    "nyc": "^7.1.0",
    "tape": "^4.6.0"
  },
  "scripts": {
